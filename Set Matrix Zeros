#include <bits/stdc++.h>

void setZeros(vector<vector<int>> &matrix)
{
	int n=matrix.size();
	int m=matrix[0].size();
	// vector<int> row(n,0),col(m,0);
    int row=1;
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<m;j++)
		{
			if(matrix[i][j]==0)
			{
				matrix[i][0]=0;
				matrix[0][j]=0;
			}
		}
	}
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<m;j++)
		{
			if(matrix[i][0]==0||matrix[0][j]==0)
			{
				if(i==0)
				{
					if()
				}

				matrix[i][j]=0;
			}
		}
	}
}
