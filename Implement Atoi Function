#include <bits/stdc++.h> 
int atoi(string str) {
   
    //bool flag= true;
    int res=0;
    for(int i=0;i<str.length();i++)
    {
        if(str[i]>='0'&&str[i]<='9')
        {
           int num=str[i]-'0';
           res= res*10+ num;
           
        }
    }
    if(str[0]=='-')
    res= (-1)*res;

    return res;
    
}
